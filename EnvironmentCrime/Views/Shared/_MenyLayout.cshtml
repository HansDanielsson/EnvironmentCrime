    {
      <li><a asp-controller="Manager" asp-action="StartManager" class="reports">Start(Ärenden)</a></li>
    }
    else if ((string?)TempData["User"] == "handläggare")
    {
      <li><a asp-controller="Investigator" asp-action="StartInvestigator" class="reports">Start(Ärenden)</a></li>
    }
    else
    {
      <li><a asp-controller="Coordinator" asp-action="StartCoordinator" class="reports">Start(Ärenden)</a></li>
